local player = game.Players.LocalPlayer
local mouse = player:GetMouse()
local UIS = game:GetService("UserInputService")
local RS = game:GetService("RunService")
local TS = game:GetService("TweenService")
local Players = game:GetService("Players")
local Workspace = game.Workspace
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Lighting = game:GetService("Lighting")
local HttpService = game:GetService("HttpService")

--=== –†–ê–°–®–ò–†–ï–ù–ù–ê–Ø –°–ò–°–¢–ï–ú–ê –í–ê–ú–ü–ò–†–°–ö–ò–• –°–ü–û–°–û–ë–ù–û–°–¢–ï–ô ===--
local VampirePowers = {
    -- –ù–æ—á–Ω–æ–µ –∑—Ä–µ–Ω–∏–µ
    EnhancedVision = {
        Enabled = false,
        ThermalSight = false,
        SoulTracker = false,
        PreyHistory = {},
        ThreatAssessment = true,
        WallHack = false,
        VisionRange = 200
    },
    
    -- –ë–æ–µ–≤—ã–µ —Å–ø–æ—Å–æ–±–Ω–æ—Å—Ç–∏
    CombatArts = {
        AutoKill = false,
        SmartAim = false,
        Prediction = true,
        SilentKill = false,
        Backstab = true,
        ComboMultiplier = 1.0,
        KillStreak = 0
    },
    
    -- –ü–µ—Ä–µ–¥–≤–∏–∂–µ–Ω–∏–µ
    Movement = {
        VampireSpeed = false,
        BatSwarm = false,
        ShadowWalk = false,
        WallClimb = false,
        Teleport = false,
        PhaseShift = false,
        SpeedBoost = 50
    },
    
    -- –ú–∞–≥–∏—á–µ—Å–∫–∏–µ —Å–ø–æ—Å–æ–±–Ω–æ—Å—Ç–∏
    DarkMagic = {
        MindControl = false,
        TimeSlow = false,
        Invisibility = false,
        CloneDecoy = false,
        FearAura = false,
        BloodDrain = false
    },
    
    -- AI —Å–∏—Å—Ç–µ–º—ã
    ArtificialIntelligence = {
        StrategyAI = false,
        ThreatAnalyzer = false,
        AutoAdapt = false,
        LearningMode = false,
        DecisionMaker = false
    }
}

--=== –£–õ–£–ß–®–ï–ù–ù–ê–Ø –°–ò–°–¢–ï–ú–ê ESP –° –ò–ò ===--
local AdvancedVampireVision = {
    Objects = {},
    Connections = {},
    LastUpdate = 0,
    UpdateInterval = 0.3
}

local function calculateThreatLevel(targetPlayer)
    if not targetPlayer.Character then return 0 end
    
    local threat = 0
    local char = targetPlayer.Character
    local distance = (player.Character.PrimaryPart.Position - char.PrimaryPart.Position).Magnitude
    
    -- –û—Ü–µ–Ω–∫–∞ –æ—Ä—É–∂–∏—è
    if char:FindFirstChild("Knife") then
        threat = threat + 80
    elseif char:FindFirstChild("Gun") then
        threat = threat + 60
    end
    
    -- –û—Ü–µ–Ω–∫–∞ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏—è
    if distance < 10 then threat = threat + 40
    elseif distance < 20 then threat = threat + 20
    elseif distance < 30 then threat = threat + 10 end
    
    -- –û—Ü–µ–Ω–∫–∞ –ø–æ–≤–µ–¥–µ–Ω–∏—è
    local velocity = char.PrimaryPart.Velocity.Magnitude
    if velocity > 20 then threat = threat + 15 end
    
    return math.min(threat, 100)
end

local function createAdvancedESP(targetPlayer)
    local char = targetPlayer.Character
    if not char or not char.PrimaryPart then return end
    
    local threatLevel = calculateThreatLevel(targetPlayer)
    local role = "Innocent"
    local color = Color3.fromRGB(0, 255, 0)
    
    if char:FindFirstChild("Knife") then
        role = "KILLER"
        color = Color3.fromRGB(255, 0, 0)
    elseif char:FindFirstChild("Gun") then
        role = "SHERIFF" 
        color = Color3.fromRGB(0, 100, 255)
    end
    
    -- –£–º–Ω—ã–π —Ö–∞–π–ª–∞–π—Ç —Å –≥—Ä–∞–¥–∏–µ–Ω—Ç–æ–º —É–≥—Ä–æ–∑—ã
    local highlight = Instance.new("Highlight")
    highlight.Adornee = char
    highlight.FillColor = color
    highlight.OutlineColor = color
    highlight.FillTransparency = 0.8 - (threatLevel / 200)
    highlight.OutlineTransparency = 0
    highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
    highlight.Parent = char
    
    -- –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏–æ–Ω–Ω–∞—è –ø–∞–Ω–µ–ª—å —Å –¥–µ—Ç–∞–ª—è–º–∏
    local infoBillboard = Instance.new("BillboardGui")
    infoBillboard.Size = UDim2.new(0, 250, 0, 80)
    infoBillboard.StudsOffset = Vector3.new(0, 3.5, 0)
    infoBillboard.AlwaysOnTop = true
    infoBillboard.Adornee = char.PrimaryPart
    infoBillboard.Parent = char
    
    local infoFrame = Instance.new("Frame")
    infoFrame.Size = UDim2.new(1, 0, 1, 0)
    infoFrame.BackgroundColor3 = Color3.fromRGB(10, 0, 5)
    infoFrame.BackgroundTransparency = 0.3
    infoFrame.BorderSizePixel = 0
    infoFrame.Parent = infoBillboard
    
    -- –ò–º—è –∏ —Ä–æ–ª—å
    local nameLabel = Instance.new("TextLabel")
    nameLabel.Size = UDim2.new(1, 0, 0.4, 0)
    nameLabel.BackgroundTransparency = 1
    nameLabel.Text = targetPlayer.Name
    nameLabel.TextColor3 = color
    nameLabel.TextSize = 12
    nameLabel.Font = Enum.Font.GothamBold
    nameLabel.Parent = infoFrame
    
    local roleLabel = Instance.new("TextLabel")
    roleLabel.Size = UDim2.new(1, 0, 0.3, 0)
    roleLabel.Position = UDim2.new(0, 0, 0.4, 0)
    roleLabel.BackgroundTransparency = 1
    roleLabel.Text = role .. " | Threat: " .. threatLevel .. "%"
    roleLabel.TextColor3 = color
    roleLabel.TextSize = 10
    roleLabel.Font = Enum.Font.Gotham
    roleLabel.Parent = infoFrame
    
    -- –î–∏–Ω–∞–º–∏—á–µ—Å–∫–∞—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è
    local dataLabel = Instance.new("TextLabel")
    dataLabel.Size = UDim2.new(1, 0, 0.3, 0)
    dataLabel.Position = UDim2.new(0, 0, 0.7, 0)
    dataLabel.BackgroundTransparency = 1
    dataLabel.TextColor3 = color
    dataLabel.TextSize = 9
    dataLabel.Font = Enum.Font.Gotham
    dataLabel.Parent = infoFrame
    
    -- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –¥–∞–Ω–Ω—ã—Ö –≤ —Ä–µ–∞–ª—å–Ω–æ–º –≤—Ä–µ–º–µ–Ω–∏
    local updateConnection = RS.Heartbeat:Connect(function()
        if char and char.PrimaryPart and player.Character and player.Character.PrimaryPart then
            local distance = (player.Character.PrimaryPart.Position - char.PrimaryPart.Position).Magnitude
            local health = char.Humanoid.Health
            local maxHealth = char.Humanoid.MaxHealth
            
            dataLabel.Text = string.format("Dist: %.1f | HP: %d/%d", distance, health, maxHealth)
        end
    end)
    
    table.insert(AdvancedVampireVision.Objects, highlight)
    table.insert(AdvancedVampireVision.Objects, infoBillboard)
    table.insert(AdvancedVampireVision.Connections, updateConnection)
end

local function updateAdvancedVampireVision()
    if not VampirePowers.EnhancedVision.Enabled then return end
    
    -- –û–≥—Ä–∞–Ω–∏—á–µ–Ω–∏–µ —á–∞—Å—Ç–æ—Ç—ã –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è
    if tick() - AdvancedVampireVision.LastUpdate < AdvancedVampireVision.UpdateInterval then
        return
    end
    AdvancedVampireVision.LastUpdate = tick()
    
    -- –û—á–∏—Å—Ç–∫–∞ —Å—Ç–∞—Ä—ã—Ö –æ–±—ä–µ–∫—Ç–æ–≤
    for _, obj in pairs(AdvancedVampireVision.Objects) do
        pcall(function() obj:Destroy() end)
    end
    for _, conn in pairs(AdvancedVampireVision.Connections) do
        pcall(function() conn:Disconnect() end)
    end
    
    AdvancedVampireVision.Objects = {}
    AdvancedVampireVision.Connections = {}
    
    -- –°–æ–∑–¥–∞–Ω–∏–µ ESP –¥–ª—è –≤—Å–µ—Ö –∏–≥—Ä–æ–∫–æ–≤
    for _, targetPlayer in ipairs(Players:GetPlayers()) do
        if targetPlayer ~= player and targetPlayer.Character then
            createAdvancedESP(targetPlayer)
        end
    end
end

--=== –£–õ–£–ß–®–ï–ù–ù–ê–Ø –°–ò–°–¢–ï–ú–ê –ê–í–¢–û-–ê–ò–ú–ê ===--
local AdvancedAim = {
    Target = nil,
    Smoothing = 0.3,
    Prediction = true,
    FOV = 30,
    Priority = "Threat" -- Threat, Distance, Role
}

local function findOptimalTarget()
    local bestTarget = nil
    local bestScore = -math.huge
    
    for _, targetPlayer in ipairs(Players:GetPlayers()) do
        if targetPlayer ~= player and targetPlayer.Character and targetPlayer.Character.PrimaryPart then
            local char = targetPlayer.Character
            local score = 0
            
            -- –û—Ü–µ–Ω–∫–∞ –ø–æ —É–≥—Ä–æ–∑–µ
            if AdvancedAim.Priority == "Threat" then
                score = calculateThreatLevel(targetPlayer)
            end
            
            -- –û—Ü–µ–Ω–∫–∞ –ø–æ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏—é
            local distance = (player.Character.PrimaryPart.Position - char.PrimaryPart.Position).Magnitude
            score = score + (100 - distance)
            
            -- –û—Ü–µ–Ω–∫–∞ –ø–æ —Ä–æ–ª–∏
            if char:FindFirstChild("Knife") then
                score = score + 200
            elseif char:FindFirstChild("Gun") then
                score = score + 150
            end
            
            if score > bestScore then
                bestScore = score
                bestTarget = char
            end
        end
    end
    
    return bestTarget
end

local function advancedAimBot()
    if not VampirePowers.CombatArts.SmartAim then return end
    
    local target = findOptimalTarget()
    if not target then return end
    
    local camera = Workspace.CurrentCamera
    local targetPos = target.PrimaryPart.Position
    
    -- –ü—Ä–µ–¥—Å–∫–∞–∑–∞–Ω–∏–µ –¥–≤–∏–∂–µ–Ω–∏—è
    if AdvancedAim.Prediction then
        local targetVel = target.PrimaryPart.Velocity
        targetPos = targetPos + (targetVel * 0.2)
    end
    
    -- –ü–ª–∞–≤–Ω–æ–µ –Ω–∞–≤–µ–¥–µ–Ω–∏–µ
    local currentCF = camera.CFrame
    local targetCF = CFrame.lookAt(currentCF.Position, targetPos + Vector3.new(0, 2, 0))
    camera.CFrame = currentCF:Lerp(targetCF, AdvancedAim.Smoothing)
end

--=== –£–õ–£–ß–®–ï–ù–ù–ê–Ø –°–ò–°–¢–ï–ú–ê –ü–ï–†–ï–î–í–ò–ñ–ï–ù–ò–Ø ===--
local AdvancedMovement = {
    Connections = {},
    FlyBody = nil,
    NoClip = false
}

local function enableVampireFlight()
    if not player.Character or not player.Character.PrimaryPart then return end
    
    local root = player.Character.PrimaryPart
    
    -- –°–æ–∑–¥–∞–Ω–∏–µ —Ñ–∏–∑–∏–∫–∏ –ø–æ–ª–µ—Ç–∞
    AdvancedMovement.FlyBody = Instance.new("BodyVelocity")
    AdvancedMovement.FlyBody.Velocity = Vector3.new(0, 0, 0)
    AdvancedMovement.FlyBody.MaxForce = Vector3.new(4000, 4000, 4000)
    AdvancedMovement.FlyBody.Parent = root
    
    AdvancedMovement.Connections.flight = RS.Heartbeat:Connect(function()
        if not VampirePowers.Movement.BatSwarm or not AdvancedMovement.FlyBody then return end
        
        local camera = Workspace.CurrentCamera
        local moveVector = Vector3.new()
        
        -- –£–ª—É—á—à–µ–Ω–Ω–æ–µ —É–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –ø–æ–ª–µ—Ç–æ–º
        if UIS:IsKeyDown(Enum.KeyCode.W) then 
            moveVector = moveVector + camera.CFrame.LookVector 
        end
        if UIS:IsKeyDown(Enum.KeyCode.S) then 
            moveVector = moveVector - camera.CFrame.LookVector 
        end
        if UIS:IsKeyDown(Enum.KeyCode.A) then 
            moveVector = moveVector - camera.CFrame.RightVector 
        end
        if UIS:IsKeyDown(Enum.KeyCode.D) then 
            moveVector = moveVector + camera.CFrame.RightVector 
        end
        if UIS:IsKeyDown(Enum.KeyCode.Space) then 
            moveVector = moveVector + Vector3.new(0, 1, 0) 
        end
        if UIS:IsKeyDown(Enum.KeyCode.LeftShift) then 
            moveVector = moveVector + Vector3.new(0, -1, 0) 
        end
        
        -- –ü—Ä–∏–º–µ–Ω–µ–Ω–∏–µ —Å–∫–æ—Ä–æ—Å—Ç–∏ —Å —É—á–µ—Ç–æ–º —É—Å–∫–æ—Ä–µ–Ω–∏—è
        if moveVector.Magnitude > 0 then
            AdvancedMovement.FlyBody.Velocity = moveVector.Unit * VampirePowers.Movement.SpeedBoost
        else
            AdvancedMovement.FlyBody.Velocity = Vector3.new(0, 0, 0)
        end
    end)
end

local function enableWallClimb()
    AdvancedMovement.Connections.wallClimb = RS.Heartbeat:Connect(function()
        if not VampirePowers.Movement.WallClimb then return end
        
        local char = player.Character
        if not char or not char.PrimaryPart then return end
        
        local root = char.PrimaryPart
        local ray = Ray.new(root.Position, root.CFrame.UpVector * -5)
        local hit = Workspace:FindPartOnRayWithIgnoreList(ray, {char})
        
        if hit then
            root.Velocity = Vector3.new(root.Velocity.X, 50, root.Velocity.Z)
        end
    end)
end

--=== –£–õ–£–ß–®–ï–ù–ù–ê–Ø –°–ò–°–¢–ï–ú–ê –ö–û–ú–ë–ê–¢–ê ===--
local AdvancedCombat = {
    AutoKill = false,
    KillRadius = 15,
    ComboTimer = 0,
    LastKill = 0
}

local function performAutoKill()
    if not VampirePowers.CombatArts.AutoKill then return end
    
    for _, targetPlayer in ipairs(Players:GetPlayers()) do
        if targetPlayer ~= player and targetPlayer.Character and targetPlayer.Character.PrimaryPart then
            local char = targetPlayer.Character
            local distance = (player.Character.PrimaryPart.Position - char.PrimaryPart.Position).Magnitude
            
            if distance <= AdvancedCombat.KillRadius then
                -- –ê–≤—Ç–æ-–∞—Ç–∞–∫–∞ –ø—Ä–∏ –±–ª–∏–∑–æ—Å—Ç–∏
                player.Character.HumanoidRootPart.CFrame = char.PrimaryPart.CFrame
                
                -- –°–∏–º—É–ª—è—Ü–∏—è –∞—Ç–∞–∫–∏ (–∑–∞–≤–∏—Å–∏—Ç –æ—Ç –∫–æ–Ω–∫—Ä–µ—Ç–Ω–æ–π –∏–≥—Ä—ã)
                if tick() - AdvancedCombat.LastKill > 1 then
                    AdvancedCombat.LastKill = tick()
                    AdvancedCombat.ComboTimer = tick()
                    VampirePowers.CombatArts.KillStreak = VampirePowers.CombatArts.KillStreak + 1
                end
            end
        end
    end
end

--=== –ò–ò –°–ò–°–¢–ï–ú–ê –ü–†–ò–ù–Ø–¢–ò–Ø –†–ï–®–ï–ù–ò–ô ===--
local VampireAI = {
    GameState = {},
    Decisions = {},
    Strategies = {}
}

local function analyzeGameState()
    local state = {
        PlayerRole = "Innocent",
        Threats = {},
        Allies = {},
        Objectives = {},
        SafeZones = {}
    }
    
    -- –û–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ —Ä–æ–ª–∏ –∏–≥—Ä–æ–∫–∞
    local char = player.Character
    if char then
        if char:FindFirstChild("Knife") then
            state.PlayerRole = "Murderer"
        elseif char:FindFirstChild("Gun") then
            state.PlayerRole = "Sheriff"
        end
    end
    
    -- –ê–Ω–∞–ª–∏–∑ —É–≥—Ä–æ–∑
    for _, targetPlayer in ipairs(Players:GetPlayers()) do
        if targetPlayer ~= player and targetPlayer.Character then
            local threatLevel = calculateThreatLevel(targetPlayer)
            if threatLevel > 30 then
                table.insert(state.Threats, {
                    Player = targetPlayer,
                    ThreatLevel = threatLevel,
                    Position = targetPlayer.Character.PrimaryPart.Position
                })
            end
        end
    end
    
    return state
end

local function makeAIDecision()
    if not VampirePowers.ArtificialIntelligence.StrategyAI then return end
    
    local gameState = analyzeGameState()
    
    -- –ü—Ä–∏–Ω—è—Ç–∏–µ —Ä–µ—à–µ–Ω–∏–π –Ω–∞ –æ—Å–Ω–æ–≤–µ –∞–Ω–∞–ª–∏–∑–∞
    if gameState.PlayerRole == "Murderer" then
        -- –°—Ç—Ä–∞—Ç–µ–≥–∏—è –¥–ª—è —É–±–∏–π—Ü—ã
        if #gameState.Threats > 0 then
            local highestThreat = gameState.Threats[1]
            for _, threat in ipairs(gameState.Threats) do
                if threat.ThreatLevel > highestThreat.ThreatLevel then
                    highestThreat = threat
                end
            end
            
            -- –ê–≤—Ç–æ-–ø—Ä–µ—Å–ª–µ–¥–æ–≤–∞–Ω–∏–µ —Å–∞–º–æ–π –æ–ø–∞—Å–Ω–æ–π —Ü–µ–ª–∏
            if highestThreat.ThreatLevel > 70 then
                player.Character.HumanoidRootPart.CFrame = CFrame.lookAt(
                    player.Character.HumanoidRootPart.Position,
                    highestThreat.Position
                )
            end
        end
    elseif gameState.PlayerRole == "Sheriff" then
        -- –°—Ç—Ä–∞—Ç–µ–≥–∏—è –¥–ª—è —à–µ—Ä–∏—Ñ–∞
        for _, threat in ipairs(gameState.Threats) do
            if threat.ThreatLevel > 80 then  -- –í—ã—Å–æ–∫–∞—è —É–≥—Ä–æ–∑–∞ (—É–±–∏–π—Ü–∞)
                -- –ê–≤—Ç–æ-–ø—Ä–∏—Ü–µ–ª–∏–≤–∞–Ω–∏–µ –Ω–∞ —É–±–∏–π—Ü—É
                Workspace.CurrentCamera.CFrame = CFrame.lookAt(
                    Workspace.CurrentCamera.CFrame.Position,
                    threat.Position
                )
                break
            end
        end
    else
        -- –°—Ç—Ä–∞—Ç–µ–≥–∏—è –¥–ª—è –Ω–µ–≤–∏–Ω–Ω–æ–≥–æ
        if #gameState.Threats > 0 then
            -- –ë–µ–≥—Å—Ç–≤–æ –æ—Ç —É–≥—Ä–æ–∑
            local escapeDirection = Vector3.new(0, 0, 0)
            for _, threat in ipairs(gameState.Threats) do
                local awayFromThreat = (player.Character.PrimaryPart.Position - threat.Position).Unit
                escapeDirection = escapeDirection + awayFromThreat
            end
            
            local escapePosition = player.Character.PrimaryPart.Position + (escapeDirection.Unit * 10)
            player.Character.Humanoid:MoveTo(escapePosition)
        end
    end
end

--=== –†–ê–°–®–ò–†–ï–ù–ù–´–ï –í–ò–ó–£–ê–õ–¨–ù–´–ï –≠–§–§–ï–ö–¢–´ ===--
local function applyVampireVisuals()
    if VampirePowers.EnhancedVision.ThermalSight then
        -- –¢–µ–ø–ª–æ–≤–æ–µ –∑—Ä–µ–Ω–∏–µ
        Lighting.Ambient = Color3.fromRGB(255, 100, 50)
        Lighting.Brightness = 0.7
    end
    
    if VampirePowers.EnhancedVision.WallHack then
        -- –í–∏–¥–µ–Ω–∏–µ —Å–∫–≤–æ–∑—å —Å—Ç–µ–Ω—ã
        for _, part in ipairs(Workspace:GetDescendants()) do
            if part:IsA("BasePart") and part.Transparency < 0.8 then
                part.LocalTransparencyModifier = 0.6
            end
        end
    end
end

--=== –ì–õ–ê–í–ù–´–ô –¶–ò–ö–õ –û–ë–ù–û–í–õ–ï–ù–ò–Ø ===--
RS.Heartbeat:Connect(function()
    -- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ ESP
    updateAdvancedVampireVision()
    
    -- –ê–≤—Ç–æ-–ø—Ä–∏—Ü–µ–ª–∏–≤–∞–Ω–∏–µ
    advancedAimBot()
    
    -- –ê–≤—Ç–æ-—É–±–∏–π—Å—Ç–≤–∞
    performAutoKill()
    
    -- –ò–ò –ø—Ä–∏–Ω—è—Ç–∏–µ —Ä–µ—à–µ–Ω–∏–π
    makeAIDecision()
    
    -- –í–∏–∑—É–∞–ª—å–Ω—ã–µ —ç—Ñ—Ñ–µ–∫—Ç—ã
    applyVampireVisuals()
    
    -- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∫–æ–º–±–æ-—Å–∏—Å—Ç–µ–º—ã
    if tick() - AdvancedCombat.ComboTimer > 10 then
        VampirePowers.CombatArts.KillStreak = 0
        VampirePowers.CombatArts.ComboMultiplier = 1.0
    elseif VampirePowers.CombatArts.KillStreak > 0 then
        VampirePowers.CombatArts.ComboMultiplier = 1.0 + (VampirePowers.CombatArts.KillStreak * 0.1)
    end
end)

--=== –û–ë–†–ê–ë–û–¢–ß–ò–ö–ò –°–û–ë–´–¢–ò–ô ===--
player.CharacterAdded:Connect(function(char)
    wait(2)
    
    -- –í–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ —Å–ø–æ—Å–æ–±–Ω–æ—Å—Ç–µ–π
    if VampirePowers.Movement.VampireSpeed then
        char.Humanoid.WalkSpeed = VampirePowers.Movement.SpeedBoost
    end
    
    if VampirePowers.Movement.BatSwarm then
        enableVampireFlight()
    end
    
    if VampirePowers.Movement.WallClimb then
        enableWallClimb()
    end
end)

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –≤–∞–º–ø–∏—Ä—Å–∫–∏—Ö –ø—Ä—ã–∂–∫–æ–≤
UIS.JumpRequest:Connect(function()
    if VampirePowers.Movement.BatSwarm and player.Character then
        player.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    end
end)

--=== –§–£–ù–ö–¶–ò–ò –ê–ö–¢–ò–í–ê–¶–ò–ò –°–ü–û–°–û–ë–ù–û–°–¢–ï–ô ===--
local function toggleEnhancedVision()
    VampirePowers.EnhancedVision.Enabled = not VampirePowers.EnhancedVision.Enabled
    updateAdvancedVampireVision()
end

local function toggleSmartAim()
    VampirePowers.CombatArts.SmartAim = not VampirePowers.CombatArts.SmartAim
end

local function toggleBatSwarm()
    VampirePowers.Movement.BatSwarm = not VampirePowers.Movement.BatSwarm
    if VampirePowers.Movement.BatSwarm then
        enableVampireFlight()
    elseif AdvancedMovement.FlyBody then
        AdvancedMovement.FlyBody:Destroy()
        AdvancedMovement.FlyBody = nil
    end
end

local function toggleVampireSpeed()
    VampirePowers.Movement.VampireSpeed = not VampirePowers.Movement.VampireSpeed
    if player.Character and player.Character.Humanoid then
        player.Character.Humanoid.WalkSpeed = VampirePowers.Movement.VampireSpeed and 
            VampirePowers.Movement.SpeedBoost or 16
    end
end

local function toggleStrategyAI()
    VampirePowers.ArtificialIntelligence.StrategyAI = not VampirePowers.ArtificialIntelligence.StrategyAI
end

local function toggleAutoKill()
    VampirePowers.CombatArts.AutoKill = not VampirePowers.CombatArts.AutoKill
end

